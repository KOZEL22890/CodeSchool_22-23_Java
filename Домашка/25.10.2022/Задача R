import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a= scanner.nextInt();
        int b= scanner.nextInt();
        hz(a,b);
    }
    public static void hz(int a, int b){
        int n=a-b%a;
        System.out.println((a+n)%a);
    }
}
