import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a= scanner.nextInt();
        hz(a);
    }
    public static void hz(int a){
        int sim1=a/1000*10+a/100%10;
        int sim2=a%10*10+a/10%10;
        System.out.println(sim1-sim2+1);
    }
}
