import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        int sum = 0;
        for(int i = 0;i<a;i++){
            sum+= scanner.nextInt();
        }
        System.out.println(sum>a/2?a-sum:sum);
    }
}
