import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int sum = 0;
        for(int i = 0;i<4;i++){
            sum+=scanner.nextInt();
            sum-= scanner.nextInt();
        }
        if(sum==0) System.out.println("DRAW");
        else System.out.println(sum>0?1:2);
    }
}
