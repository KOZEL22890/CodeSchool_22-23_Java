import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int min = 10001;
        int max = 0;
        for(int i = 0;i<3;i++){
            int a = scanner.nextInt();
            min = Math.min(min,a);
            max = Math.max(max,a);
        }
        System.out.println(min<94||max>727?"Error":max);
    }
}
